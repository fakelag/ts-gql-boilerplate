{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-any": false,
        "no-unsafe-any": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": [false],
        "no-string-literal": [false],
        "ordered-imports": [false],
        "curly": [false],
        "variable-name": [false],
        "indent": [true, "tabs"],
        "quotemark": [true, "single"]
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}
